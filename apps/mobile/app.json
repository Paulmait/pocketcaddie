{
  "expo": {
    "name": "SliceFix AI",
    "slug": "slicefix-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "slicefix",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0F14"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.cienrios.pocketcaddieai",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "SliceFix AI needs camera access to record your golf swing for analysis.",
        "NSPhotoLibraryUsageDescription": "SliceFix AI needs photo library access to select swing videos for analysis.",
        "NSMicrophoneUsageDescription": "SliceFix AI needs microphone access when recording swing videos."
      },
      "usesAppleSignIn": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B0F14"
      },
      "package": "com.cienrios.pocketcaddieai"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow SliceFix AI to access your camera to record swing videos."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow SliceFix AI to access your photos to select swing videos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "14bd2c65-383f-4be2-9ef2-569ce481b3d4"
      }
    }
  }
}
